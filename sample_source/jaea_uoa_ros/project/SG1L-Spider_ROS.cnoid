          type: splitter
          orientation: vertical
          sizes: [ 388, 386 ]
          children: 
            - 
              type: pane
              views: [ 1 ]
              current: 1
            - 
              type: pane
              views: [ 0, 8 ]
              current: 0
        - 
          type: splitter
          orientation: vertical
          sizes: [ 553, 221 ]
          children: 
            - 
              type: splitter
              orientation: horizontal
              sizes: [ 0, 1291 ]
              children: 
                - 
                  type: pane
