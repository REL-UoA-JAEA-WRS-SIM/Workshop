        - 
          type: splitter
          orientation: vertical
          sizes: [ 393, 393 ]
          children: 
            - 
              type: pane
              views: [ 1 ]
              current: 1
            - 
              type: pane
              views: [ 0, 8 ]
              current: 0
        - 
          type: splitter
          orientation: vertical
          sizes: [ 561, 225 ]
          children: 
            - 
              type: splitter
              orientation: horizontal
              sizes: [ 697, 880 ]
              children: 
                - 
                  type: pane
